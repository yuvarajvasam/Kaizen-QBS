{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/api/generate-pdf/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "app/api/health/route.ts": {
      "maxDuration": 10
    },
    "app/api/test/route.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}
